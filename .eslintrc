{
"settings": {
	"import/resolver": {
		  webpack: {
			  config: 'config/webpack.common.js'
		  },
		},
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"sourceType": "module",
		"allowImportExportEverywhere": true,
		"codeFrame": false
	},
	"extends": ["standard", "airbnb-base"],
	"rules":{
		"no-console": 0,
		"indent": ["error", "tab", { "SwitchCase": 1 }],
		"no-tabs": 0,
		"consistent-return": 1,
	},
	"globals": {
		"document": false,
		"$": false,
		"window": false,
		"self": false,
		"log": false,
		"data": false,
		"google": false,
		"SITE": false,
		"IntersectionObserver": false,
		"history": false,
		"ResultFilters": false,
		"fetch": false,
		"TweenLite": false,
		"TimelineLite": false,
		"Cookie": false,
		"AccordionStyle": false
	}
}
